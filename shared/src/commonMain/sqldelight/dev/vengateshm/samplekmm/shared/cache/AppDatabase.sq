CREATE TABLE StockExchange (
    name TEXT NOT NULL,
    code TEXT NOT NULL,
    country TEXT NOT NULL,
    timezone TEXT
);

insertStockExchange:
INSERT INTO StockExchange(name, code, country, timezone)
VALUES(?,?,?,?);

removeAllStockExchanges:
DELETE FROM StockExchange;

selectAllStockExchangesInfo:
SELECT StockExchange.*
FROM StockExchange;